#:import FadeTransition kivy.uix.screenmanager.FadeTransition
#:import sm kivy.uix.screenmanager

<Wind_Menu>:
    name:"screen_a"
    BoxLayout:
        orientation: "vertical"
        canvas: 
            Color:  
                rgb: 0.3,1.5,5,0.5
            Rectangle:
                source:'fondo.png'
                pos: self.pos
                size: self.size
        RelativeLayout:
            size_hint: None,None
            anchor_x:'center'
            anchor_y:'top'
            size: 350,750
            Label:
                markup: True
                text: "[font=./STARFISH]Select an Option:[/font]" 
                font_size: 22
                size_hint: None,None
                size: 700,30
                pos: 600, 900        
                color: [1,1,1,1]
            Button:
                markup: True
                text: "[font=STARFISH][size=16]Buscar Juego[/size][/font]" 
                size_hint: None,None
                size: 400,100
                pos: 750,700
                on_release: 
                on_press: app.root.current = "Lista de juegos"
                background_color: [0,0,200,0.5]     
            Button:
                markup: True
                text: "[font=STARFISH][size=16]Consolas[/size][/font]" 
                size_hint: None,None
                size: 400,100
                pos: 750,550
                on_release:
                on_press: app.root.current = "Lista de consolas"
                background_color: [0,0,200,0.5]                    
            Button:
                markup: True
                text: "[font=STARFISH][size=16]Ultimos jugados[/size][/font]" 
                size_hint: None,None
                size: 400,100
                pos: 750,400
                on_release:
                on_press: app.root.current = "Ultimos jugados"
                background_color: [0,0,200,0.5]                
                
            Button:
                markup: True
                text: "[font=STARFISH][size=16]Atras[/size][/font]" 
                size_hint: None,None
                size: 150,50
                pos: 100,175
                background_color: [0.3,1,1,0.5]
                on_release: 
                on_press: root.manager.current = "Pregunta"
                #root.manager.current = "left"
            
                    

<Wind_Pregunta>:
    Screen:
        name:"Pregunta"
        BoxLayout:
            orientation: "vertical"
            canvas: 
                Color:  
                    rgb: 0,0.5,1,0.5
                Rectangle:
                    pos: self.pos
                    size: self.size
            RelativeLayout:
                size_hint: None,None
                Label:
                    markup: True
                    text: "[font=./STARFISH]Seleccione la dimension de su pantalla:[/font]" 
                    font_size: 22
                    size_hint: None,None
                    size: 700,30
                    pos: 600, 900        
                    color: [1,1,1,1]
                Button:
                    markup: True
                    text: "[font=STARFISH][size=16]32' pulgadas[/size][/font]"
                    size_hint: None,None
                    size: 450,100
                    pos: 700,750
                    on_release: 
                    on_press: app.root.current = "screen_a"
                    background_color: [0,0,200,0.5]
                    on_release: root.manager.FadeTransition = "left"
                Button:
                    markup: True
                    text: "[font=STARFISH][size=16]40' pulgadas[/size][/font]"
                    size_hint: None,None
                    size: 450,100
                    pos: 700,650
                    on_press: root.current = "screen_a"
                    background_color: [0,0,200,0.5]
                Button:
                    markup: True
                    text: "[font=STARFISH][size=16]42' pulgadas[/size][/font]"
                    size_hint: None,None
                    size: 450,100
                    pos: 700,550
                    on_press: root.current = "screen_a"
                    background_color: [0,0,200,0.5]
                Button:
                    markup: True
                    text: "[font=STARFISH][size=16]50' pulgadas[/size][/font]"
                    size_hint: None,None
                    size: 450,100
                    pos: 700,450
                    on_press: root.current = "screen_a"
                    background_color: [0,0,200,0.5]
                Button:
                    markup: True
                    text: "[font=STARFISH][size=16]60' pulgadas[/size][/font]"
                    size_hint: None,None
                    size: 450,100
                    pos: 700,350
                    on_press: root.current = "screen_a"
                    background_color: [0,0,200,0.5]
                
